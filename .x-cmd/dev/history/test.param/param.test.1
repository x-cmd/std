. param

param.example "Rebuild the page in sort order 'repo,time,update'" \
    "gt.repo.page.build --org x-bash --repo std --sort repo,time,update" \
    "Retrieve the page info" \
    "gt.repo.page.info --org x-bash --repo std"

param '
    #1   "Working Number"  =int
    #2   "Working Number"  =int
    ... "Provice repo names" =~ [[:alnum:]]+ ([[:alnum:]]+/)([[:alnum:]]+)
    argenv:org  "org-name"
        =str
    repo=hi "repo-name"
    argENV:access=public "Access code"
        =       public private all
    sort    "sort code"
        =[,]    repo time update
    show
        =~[,]   (repo)|(time)|(update)
'


echo.name(){
    for i in "$@"; do
        echo "$i: ${!i}"
    done
}

echo.name org repo access sort show

