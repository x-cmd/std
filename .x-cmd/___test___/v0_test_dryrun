# shellcheck shell=sh disable=SC2039,SC2142,SC3043

dryrun(){
    param:dsl <<A
option:
    -y|--fsdfsd|-o          "Test regex arg1"  <regex_arg1>=888  =~  "[0-9]+"
A
    param:run

    echo "y: ${y}"
}

test_dryrun(){
    @assert false dryrun _dryrun -y qwe
    @assert true dryrun _dryrun -y 123

    @assert stdout dryrun -y 777 <<A
y: 777
A

}
@assert:file:func