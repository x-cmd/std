
test_dict_load_json(){
    dict @b free
    dict_log "test1: ___X_BASH_DICT_b:${___X_BASH_DICT_b}"
    dict @b make

    local test_json='{
  "3": "c",
  "2 \" 3 ": "b",
  "1": "a",
  "2": "b"
}'

    # BUG: dict_load_json does not work in zsh with spaces in keys
    dict @b load_json "${test_json}"

    @assert stdout "dict @b json | x jq '[ . | length, .\"1\", .\"2\", .\"2 \\\" 3 \" ]'" <<A
[
  4,
  "a",
  "b",
  "b"
]
A

}

test_dict_load_json
