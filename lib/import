
___x_cmd_os_import(){
    local i

    case "$(___x_cmd_os name)" in
        darwin)
            for i in "$@"; do
                xrc "os/lib/darwin/$i"
            done
            ;;
        linux)
            for i in "$@"; do
                xrc "os/lib/linux/$i"
            done
            ;;
        win)
            for i in "$@"; do
                xrc "os/lib/win/$i"
            done
            ;;
    esac
}
