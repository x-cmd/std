# shellcheck shell=sh disable=SC3043 # xrc

# author:       Li Junhao           l@x-cmd.com
# maintainer:   Li Junhao

x log init proxy

___x_cmd_proxy(){
    xrc proxy/lib/main
    proxy "$@"
}

if ___x_cmd_is_suitable_advise_env; then
    xrc advise/latest
    advise proxy
fi

xrc setmain ___x_cmd_proxy
