# shellcheck shell=sh disable=SC2154

###
 # @Description: ___x_cmd_ps1env_reset Test
###

init_cmd
___x_cmd_ps1env_reset

___mock_get_alias() {
    if [ -n "$ZSH_VERSION" ]; then
        alias -- "$1"
    else
        alias "$1"
    fi
}

test_cmd_reset_symbol(){
    printf "%s" '1' | @assert stdout "echo 1\$___X_CMD_THEME_CONTROL_SUBENV_PROMT"
}

@assert:file:func