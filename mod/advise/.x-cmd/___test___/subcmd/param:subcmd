# shellcheck shell=sh disable=SC2039,SC2142,SC3043

param_subcmd(){
    param:subcmd    param_subcmd    \
        current     "Test current"
    param:subcmd:try
    param:run
}

param_subcmd_current(){
    param:dsl  '
options:
    --qwe       "Test qwe"      <qwe> =~ ([1-9][0-9]?|100)
'
    param:run

    printf "%s\n" "qwe: $qwe"
}

test_param_subcmd(){
    @assert stdout param_subcmd current --qwe 99 <<A
qwe: 99
A
}
@assert:file:func