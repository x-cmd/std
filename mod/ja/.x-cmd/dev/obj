
# Using context :gh
#     > ccmd :gh 3 -- curl https://github.com/x-cmd/x-cmd
#     > ccmd :gh set "${TMPDIR}/cache/gh"

        # :*)                     obj="${op#:}" ___x_cmd_ccmd_exec_obj "$@" ;;

___x_cmd_ccmd_exec_obj(){
    local filepath
    if ! filepath="$(___x_cmd_ccmd___query_path "$op")"; then
        ccmd:warn "Object Not Found: $op"
        return 1
    fi

    local X_CMD_CACHE_STORAGE_FOLDER_PATH="$filepath"
    local op="${1}"
    shift

    case "$op" in
        clear)                  ___x_cmd_ccmd_clear "$@" ;;
        invalidate|-)           ___x_cmd_ccmd_invalidate "$@" ;;
        *)                      ___x_cmd_ccmd_exec "$op" "$@" ;;
    esac
}