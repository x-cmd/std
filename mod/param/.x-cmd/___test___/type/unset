# shellcheck shell=sh

param_type      gitee     unsetType        =    aaa bbb
param_not_unset_type(){
    param:scope "gitee"
    param:dsl '
option:
    --hhh       "desc hhh"      <hhh>:unsetType="ccc"
'
    param:run
}

param_unset_type(){
    ___x_cmd_param_type unset gitee
    param:scope "gitee"
    param:dsl '
option:
    --hhh       "desc hhh"      <hhh>:unsetType="ccc"
'
    param:run
}

test_param_unset_type(){
    @assert false param_not_unset_type
    @assert true  param_unset_type
}
@assert:file:func