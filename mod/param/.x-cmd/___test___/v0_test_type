xrc param/latest

# x test "$(x wsroot)/.x-cmd/testcase/v0_test_type"
fff(){
    echo "aaa bbb"
}
param_type type1 UserGroup = owner "\"\"" admin
param_type type1 License   -  <<A
eee
fff
A
param_type type1 func = "$(fff)"
param_test_type(){
    param:scope "type1"
    param:dsl '
type:
    Access = private "" public
option:
    --aaa "desc aaa" <>:Access=public
    --bbb "desc bbb" <>:Access=

    --ccc "desc ccc" <>:License=fff
    --ddd "desc ddd" <>:UserGroup=
    --eee "desc eee" <>:func=aaa
'
    param:run

    echo "aaa:$aaa"
    echo "bbb:$bbb"
    echo "ccc:$ccc"
    echo "ddd:$ddd"
    echo "eee:$eee"
}
@assert stdout param_test_type <<A
aaa:public
bbb:
ccc:fff
ddd:
eee:aaa
A