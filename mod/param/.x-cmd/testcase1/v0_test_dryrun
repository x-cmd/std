# shellcheck shell=sh disable=SC2039,SC2142,SC3043

dryrun(){
    param:dsl <<A
option:
    -y|--fsdfsd|-o          "Test regex arg1"  <regex_arg1>=888  =~  "[0-9]+"
A
    param:run

    echo "y: ${y}"
}

test_dryrun(){
    dryrun _dryrun -y qwe
    assert stdout echo $?<<A
1
A

    dryrun _dryrun -y 123
    assert stdout echo $?<<A
0
A

    assert stdout dryrun -y 777 <<A
y: 777
A

}