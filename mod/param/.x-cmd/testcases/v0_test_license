# shellcheck shell=sh disable=SC2039,SC2142,SC3043

param_type      gitee     License   -  <<A
$(cat .x-cmd/testcases/license.txt)
A

paramtest_license(){
    param:scope     "gitee/$O"
    param:dsl       '
option:
    --pd|-d    "Test param default setting"     <>:License=""
'
    param:run
    printf "param license: %s\n" "$pd"
}

test_paramtest_license(){
    assert_stdout "paramtest_license -d BSD-2-Clause" <<A
param license: BSD-2-Clause
A
}

