# shellcheck shell=sh

ls_option_name(){
    param:dsl <<A
option:
    -y|--fsdfsd|-o          "Test regex arg1"  <regex_arg1>=888         =~  "[0-9]+"
    --license               "Test regex arg3"  <regex_arg3>="0BSD"      =   "MulanPSL-2.0" "0BSD" "AFL-3.0" "AGPL-3.0" "" "XXXX License" /[0-9]+/
    -z                      "Test regex arg4"
    -k                      "Test regex arg5"
        <arg1>="abc"               =   "abc"   "cde"   "def"
        <arg2>=666
    #1|--regex_arg3|-s      "Repo name 1"       <rest_arg_1>
A
    param:run
}

test_ls_option_name(){
    @assert stdout ls_option_name _ls_option_name <<A
y
license
k
A
}
@assert:file:func