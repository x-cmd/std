
cat "${1:?word list}" | awk '{
    print length($0) "\t" $0
}' | sort -n -k 1 | awk -f change.awk | sort -nk 1 -nk2  | awk '
BEGIN {
    last = -1
}
{
    if ("" last == $1) {
        printf(" %s", $3)
    } else {
        if (last != -1) printf("\n")
        last = $1
        printf("%s\t%s", $1, $3)
    }
}' >"$1.result.txt"
