[
    {
        "pattern": [ "repo1" ],
        "id": "repo1",
        "mirrorOf": "central",
        "name": "official maven repo1",
        "url": "http://repo1.maven.org/maven2/"
    },
    {
        "pattern": [ "repo2" ],
        "id": "repo2",
        "mirrorOf": "central",
        "name": "official maven repo2",
        "url": "http://repo2.maven.org/maven2/"
    },
    {
        "pattern": [ "uk", "uk-repo" ],
        "id": "uk",
        "mirrorOf": "central",
        "name": "official maven repo in uk",
        "url": "http://uk.maven.org/maven2/"
    },
    {
        "pattern": [ "ali", "aliyun" ],
        "id": "nexus-aliyun",
        "mirrorOf": "*",
        "name": "Nexus aliyun",
        "url": "http://maven.aliyun.com/nexus/content/groups/public"
    },
    {
        "pattern": [ "jboss" ],
        "id": "jboss-public-repository-group",
        "mirrorOf": "central",
        "name": "JBoss Public Repository Group",
        "url": "http://repository.jboss.org/nexus/content/groups/public"
    },
    {
        "pattern": [],
        "id": "net-cn",
        "mirrorOf": "central",
        "name": "official maven repo in net.cn",
        "url": "http://maven.net.cn/content/groups/public/"
    },
    {
        "pattern": [],
        "id": "ibiblio",
        "mirrorOf": "central",
        "name": "ibiblio Mirror of http://repo1.maven.org/maven2/",
        "url": "http://mirrors.ibiblio.org/pub/mirrors/maven2/"
    }
]