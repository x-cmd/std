p="$(x ws root)"

function work(){
    cat <<A
{
    "a": 1,
    "b": 2,
    "c": {
        "d": 1,
        "e": [ 1, "two", "3" ]
    }
}
A
}

# function work(){
#     cat <<A
# {
#     "a": 1,
#     "b": 2
# }
# A
# }


work | awk -v RS="" -f "$p/lib/json.awk" -f "$p/lib/jqparse.awk" -f "_jqparse.awk"
# work | awk -v RS="" -f "./lib/default.awk" -f "./lib/json.awk" -f "./lib/jqparse.awk" -f "./.x-cmd/___test___/_jqparse.awk"