
test_dict_premove(){
    test_dict_setup
    @assert false dict @b remove 4
    @assert true dict @b has 2
    dict @b drop 2
    @assert false dict @b has 2
}

test_dict_pdrop(){
    test_dict_setup
    @assert true dict @b drop [1-3]+
    @assert false dict @b has 1
    @assert false dict @b has 5
}

test_dict_premove
test_dict_pdrop
