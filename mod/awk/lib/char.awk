

function char_at(s, idx) {
    return substr(s, idx, 1)
}

function char_is_number(c) {
    return ord_is_number(_ord_[c])
}

function char_is_letter(c) {
    return ord_is_number(_ord_[c])
}

function char_is_uppercase(c) {
    return ord_is_number(_ord_[c])
}

function char_is_lowercase(c) {
    return ord_is_number(_ord_[c])
}

